{
  "name": "bittorrent-sync",
  "version": "0.0.3",
  "description": "A simple wrapper for the BitTorrent Sync API",
  "keywords": [
    "bittorrent",
    "btsync",
    "api",
    "p2p",
    "peer2peer"
  ],
  "homepage": "https://github.com/yannickcr/node-bittorrent-sync",
  "main": "./lib/bittorrent-sync.js",
  "scripts": {
    "test": "node ./node_modules/mocha/bin/mocha test --reporter=spec",
    "coverage": "node ./node_modules/mocha/bin/mocha test --require blanket --reporter=html-cov > coverage.html",
    "checkstyle": "node ./node_modules/jscs/bin/jscs .",
    "blanket": {
      "pattern": [
        "/lib/bittorrent-sync.js"
      ]
    }
  },
  "dependencies": {
    "request": "~2.29.0"
  },
  "devDependencies": {
    "mocha": "~1.15.1",
    "mocha-lcov-reporter": "~0.0.1",
    "blanket": "~1.1.5",
    "coveralls": "~2.6.0",
    "nock": "~0.25.0",
    "chai": "~1.8.1",
    "chai-json-schema": "~1.0.10",
    "jscs": "~1.2.2"
  },
  "engines": {
    "node": ">=0.8.0"
  },
  "bugs": {
    "url": "https://github.com/yannickcr/node-bittorrent-sync/issues"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/yannickcr/node-bittorrent-sync.git"
  },
  "author": "Yannick Croissant (https://github.com/yannickcr)",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://raw.github.com/yannickcr/node-bittorrent-sync/master/LICENSE"
    }
  ]
}
